#!/data/work/PyCharm_Project/mcp-sentry-custom/test_env/bin/python3
# -*- coding: utf-8 -*-
import sys
from mcp_sentry_custom import main
if __name__ == "__main__":
    if sys.argv[0].endswith("-script.pyw"):
        sys.argv[0] = sys.argv[0][:-11]
    elif sys.argv[0].endswith(".exe"):
        sys.argv[0] = sys.argv[0][:-4]
    sys.exit(main())
