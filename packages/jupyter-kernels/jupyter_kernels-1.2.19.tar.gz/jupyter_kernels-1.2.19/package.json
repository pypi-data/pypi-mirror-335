  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "disabledExtensions": [],
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyter_kernels"
        }
      }
    },
    "extension": "lib/jupyterlab/index.js",
    "outputDir": "jupyter_kernels/labextension",
    "schemaDir": "schema",
    "webpackConfig": "./webpack.lab-config.js",
    "sharedPackages": {
      "react": {
        "bundled": false,
        "singleton": true
      },
      "react-dom": {
        "bundled": false,
        "singleton": true
      }
    }
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab==4.1.0b0",
        "jlpm"
      ],
      "before-build-python": [
        "jlpm clean:all"
      ]
    }
  }
