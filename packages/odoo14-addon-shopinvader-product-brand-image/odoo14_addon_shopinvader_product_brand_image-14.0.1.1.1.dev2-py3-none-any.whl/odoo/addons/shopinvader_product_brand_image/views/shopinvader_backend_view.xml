<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record model="ir.ui.view" id="shopinvader_backend_form_view">
        <field
            name="name"
        >shopinvader.backend.form (in shopinvader_product_brand_image)</field>
        <field name="model">shopinvader.backend</field>
        <field
            name="inherit_id"
            ref="shopinvader_image.shopinvader_backend_view_form"
        />
        <field name="arch" type="xml">
            <field name="shopinvader_category_resize_ids" position="after">
                <field name="shopinvader_brand_resize_ids" widget="many2many_tags" />
            </field>

        </field>
    </record>

</odoo>
